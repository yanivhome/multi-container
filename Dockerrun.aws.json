{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name" : "client",
      "image" : "yanivhome1/multi-client",
      "hostname" : "client",
      "essentinal" : false
    },
    {
      "name" : "server",
      "image" : "yanivhome1/multi-server",
      "hostname" : "api",
      "essentinal" : false
    },
    {
      "name" : "worker",
      "image" : "yanivhome1/multi-worker",
      "hostname" : "worker",
      "essentinal" : false
    },
    {
      "name" : "nginx",
      "image" : "yanivhome1/multi-mginx",
      "hostname" : "nginx",
      "essentinal" : true,
      "portMappings" : [
      	{
      		"hostPort": 80,
      		"containerPort": 80
      	}
      ],
      "links": ["client", "server"]
    }
  ]
 }